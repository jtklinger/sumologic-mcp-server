{
  "mcpServers": {
    "sumologic": {
      "command": "python",
      "args": ["-m", "sumologic_mcp_server.server"],
      "cwd": "YOUR_PATH_HERE",
      "env": {
        "SUMO_ACCESS_ID": "${SUMO_ACCESS_ID}",
        "SUMO_ACCESS_KEY": "${SUMO_ACCESS_KEY}",
        "SUMO_ENDPOINT": "${SUMO_ENDPOINT:-https://api.sumologic.com/api}"
      }
    }
  }
}
